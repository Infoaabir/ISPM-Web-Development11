.heading {
	font-family: "Times New Roman", Times, serif;
	font-size: 18pt;
	line-height: normal;
	color: #656CB0;
	text-decoration: none;
}
.text {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #333333;
	height: 20px;

}
.subheding {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10pt;
	color: #333333;
	font-weight: bold;
}
.line {
	background-color: #D6D6D6;
}
.boxline {
	background-color: #666699;
	height: 2px;
}
.hightext {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #666699;
	font-weight: bold;
}
.niddlehead {
	font-family: "Times New Roman", Times, serif;
	font-size: 22px;
	color: #666699;
	background-color: #D6D6EB;
	font-style: italic;


}
.niddlehead2 {
	font-family: "Times New Roman", Times, serif;
	font-size: 20px;
	color: #666699;
	font-style: italic;
	margin: 10px;
	padding: 10px;

}
.textacc {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #333333;
	text-align: right;
	padding: 2px;



}
.textsym {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #333333;
	text-transform: lowercase;

}
a:hover {
	color: #FF3300;
}
.headingpop {

	font-family: "Times New Roman", Times, serif;
	font-size: 14pt;
	line-height: normal;
	color: #656CB0;
	text-decoration: none;
}
.textsmall {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	color: #333333;
	height: 20px;
	padding: 4px;

}
.nolink {
	cursor: default;
}
